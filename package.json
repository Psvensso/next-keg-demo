{
  "name": "keg-webb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "set NODE_ENV=test&&next dev --port=3001",
    "start": "next start",
    "lint": "next lint",
    "bump": "npx npm-check-updates -i --format group --install never",
    "db:reset": "rimraf src/db/generated/ && prisma migrate dev --name init --schema=prisma/schema.prisma && npm run db:seed",
    "db:migrate": "prisma migrate deploy --schema=prisma/schema.prisma",
    "db:generate": "prisma generate",
    "db:seed": "tsx ./prisma/seed/seed.ts",
    "db:prepare:prod": "npm run db:migrate && npm run db:generate && npm run db:seed",
    "build:ci": "npm run db:migrate && npm run db:generate && npm run db:seed && next build",
    "build": "next build"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.16.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@prisma/client": "^6.7.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "next": "15.4.0-canary.17",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-icons": "^5.5.0",
    "react-use": "^17.6.0",
    "strip-bom-stream": "^5.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "csv-parser": "^3.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "github-slugger": "^2.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "prisma": "^6.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
